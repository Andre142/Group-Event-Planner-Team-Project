<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Project</display-name>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>csci310.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>csci310.servlets.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchEventServlet</servlet-name>
        <servlet-class>csci310.servlets.SearchEventServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchUserServlet</servlet-name>
        <servlet-class>csci310.servlets.SearchUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendProposalServlet</servlet-name>
        <servlet-class>csci310.servlets.SendProposalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/auth/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/auth/signUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchEventServlet</servlet-name>
        <url-pattern>/search/event</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchUserServlet</servlet-name>
        <url-pattern>/search/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendProposalServlet</servlet-name>
        <url-pattern>/send-proposal</url-pattern>
    </servlet-mapping>
</web-app>
